{
  "name": "sap.ui.core.sample.RoutingNestedComponent",
  "version": "1.0.0",
  "author": "SAP SE",
  "description": "Sample: Routing with nested components consuming data from a mocked OData V2 service",
  "scripts": {
    "start": "ui5 serve",
    "build": "ui5 build",
    "start2": "fiori run --open 'index.html'",
		"start-local": "fiori run --config ./ui5-local.yaml --open 'index.html'",
		"build2": "ui5 build -a --clean-dest --include-task=generateManifestBundle generateCachebusterInfo",
		"deploy": "fiori verify",
		"deploy-config": "fiori add deploy-config",
		"unit-tests": "fiori run --open test/unit/unitTests.qunit.html",
		"start-mock": "fiori run --open test/mockServer.html",
		"int-tests": "fiori run --open test/integration/opaTests.qunit.html"
  },
  "dependencies": {
    "@openui5/sap.m": "^1",
    "@openui5/sap.tnt": "^1",
    "@openui5/sap.ui.core": "^1",
    "@openui5/themelib_sap_fiori_3": "^1",
    "@sap/ux-ui5-tooling"
  },
  "devDependencies": {
    "@ui5/cli": "^2.0.0",
    "@sap/ux-ui5-tooling": "1",
		"@types/openui5": "^1.40.1",
		"@ui5/fs": "2.0.1",
		"@ui5/logger": "2.0.0"
  }
}
